<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE SPEC SYSTEM "../dat/SPECForward.dtd">
<SPEC>
	<MODULE>
		<MTYPE>
			<NAME VALUE="CLIENTREQ" />
			<NAME VALUE="ACK" />
			<NAME VALUE="REPLREQ1" />
			<NAME VALUE="REPLREQ2" />
			<NAME VALUE="SYNC1" />
			<NAME VALUE="SYNC2" />
			<NAME VALUE="STORE1" />
			<NAME VALUE="STORE2" />
		</MTYPE>
	</MODULE>
	<MODULE>
		<PROCTYPE>
			<NAME VALUE="CLIENT" />
			<SEQUENCE>
				<STEP>
					<DECLLIST>
						<DECL>
							<TYPENAME>
								<NUM />
							</TYPENAME>
							<IVAR>
								<NAME VALUE="RECVD1" />
							</IVAR>
						</DECL>
						<DECL>
							<TYPENAME>
								<NUM />
							</TYPENAME>
							<IVAR>
								<NAME VALUE="RECVD2" />
							</IVAR>
						</DECL>
						<DECL>
							<TYPENAME>
								<NUM />
							</TYPENAME>
							<IVAR>
								<NAME VALUE="STORECOMPL1" />
							</IVAR>
						</DECL>
						<DECL>
							<TYPENAME>
								<NUM />
							</TYPENAME>
							<IVAR>
								<NAME VALUE="STORECOMPL2" />
							</IVAR>
						</DECL>
					</DECLLIST>
				</STEP>
				<STEP>
					<STMT>
						<SEND>
							<NAME VALUE="CLIENTREQ" />
						</SEND>
					</STMT>
				</STEP>
				<STEP>
					<STMT>
						<RECEIVE>
							<NAME VALUE="ACK" />
						</RECEIVE>
					</STMT>
				</STEP>
				<STEP>
					<STMT>
						<ASSERT EVAL="TRUE" LABEL="L1" VARUSE="STORECOMPL1,STORECOMPL2">
							<EXPR>
								<VARREF>
									<NAME VALUE="STORECOMPL1" />
								</VARREF>
								<OP VALUE="AND" />
								<CONST VALUE="STORECOMPL2" />
							</EXPR>
						</ASSERT>
					</STMT>
				</STEP>

			</SEQUENCE>
		</PROCTYPE>

	</MODULE>

	<MODULE>
		<PROCTYPE>
			<NAME VALUE="STORAGENODE1" />
			<SEQUENCE>
				<STEP>
					<STMT>
						<DO>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="REPLREQ1" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<SEND>
												<NAME VALUE="STORE1" />
											</SEND>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
							<!-- timeout option -->
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<IF>
												<OPTION>
													<EXPR>
														<VARREF>
															<NAME VALUE="STORECOMPL1" />
														</VARREF>
														<OP VALUE="==" />
														<VARREF>
															<NAME VALUE="1,1" />
														</VARREF>
													</EXPR>
													<SEQUENCE>
														<STEP>
															<STMT>
																<SEND>
																	<NAME VALUE="SYNC1" />
																</SEND>
															</STMT>
														</STEP>
													</SEQUENCE>
												</OPTION>
												<ELSE>
													<SEQUENCE>
														<STEP>
															<STMT>
																<EXPR>
																	<CONST VALUE="1" />
																</EXPR>
															</STMT>
														</STEP>
													</SEQUENCE>
												</ELSE>
											</IF>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
						</DO>
					</STMT>
				</STEP>
			</SEQUENCE>
		</PROCTYPE>
	</MODULE>

	<MODULE>
		<PROCTYPE>
			<NAME VALUE="STORAGENODE2" />
			<SEQUENCE>
				<STEP>
					<STMT>
						<DO>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="REPLREQ2" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<SEND>
												<NAME VALUE="STORE2" />
											</SEND>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
							<!-- timeout option -->
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<IF>
												<OPTION>
													<EXPR>
														<VARREF>
															<NAME VALUE="STORECOMPL2" />
														</VARREF>
														<OP VALUE="==" />
														<VARREF>
															<NAME VALUE="1,1" />
														</VARREF>
													</EXPR>
													<SEQUENCE>
														<STEP>
															<STMT>
																<SEND>
																	<NAME VALUE="SYNC2" />
																</SEND>
															</STMT>
														</STEP>
													</SEQUENCE>
												</OPTION>
												<ELSE>
													<SEQUENCE>
														<STEP>
															<STMT>
																<EXPR>
																	<CONST VALUE="1" />
																</EXPR>
															</STMT>
														</STEP>
													</SEQUENCE>
												</ELSE>
											</IF>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
						</DO>
					</STMT>
				</STEP>
			</SEQUENCE>
		</PROCTYPE>
	</MODULE>

	<MODULE>
		<PROCTYPE>
			<NAME VALUE="STORE" />
			<SEQUENCE>
				<STEP>
					<STMT>
						<DO>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="STORE1" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<ASSIGN>
												<VARREF>
													<NAME VALUE="STORECOMPL1" />
												</VARREF>
												<EXPR>
													<VARREF>
														<NAME VALUE="1,1" />
													</VARREF>
												</EXPR>
											</ASSIGN>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="STORE2" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<ASSIGN>
												<VARREF>
													<NAME VALUE="STORECOMPL2" />
												</VARREF>
												<EXPR>
													<VARREF>
														<NAME VALUE="1,1" />
													</VARREF>
												</EXPR>
											</ASSIGN>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
						</DO>
					</STMT>
				</STEP>
			</SEQUENCE>
		</PROCTYPE>
	</MODULE>



	<MODULE>
		<PROCTYPE>
			<NAME VALUE="SERVER" />
			<SEQUENCE>
				<STEP>
					<STMT>
						<DO>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="CLIENTREQ" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<SEND>
												<NAME VALUE="REPLREQ1" />
											</SEND>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<SEND>
												<NAME VALUE="REPLREQ2" />
											</SEND>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="SYNC1" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<IF>
												<OPTION>
													<EXPR>
														<VARREF>
															<NAME VALUE="RECVD1" />
														</VARREF>
														<OP VALUE="==" />
														<VARREF>
															<NAME VALUE="1,0" />
														</VARREF>
													</EXPR>
													<SEQUENCE>
														<STEP>
															<STMT>
																<ASSIGN>
																	<VARREF>
																		<NAME VALUE="RECVD1" />
																	</VARREF>
																	<EXPR>
																		<VARREF>
																			<NAME VALUE="1,1" />
																		</VARREF>
																	</EXPR>
																</ASSIGN>
															</STMT>
														</STEP>
														<STEP>
															<STMT>
																<IF>
																	<OPTION>
																		<EXPR>
																			<VARREF>
																				<NAME VALUE="RECVD2" />
																			</VARREF>
																			<OP VALUE="==" />
																			<VARREF>
																				<NAME VALUE="1,1" />
																			</VARREF>
																		</EXPR>
																		<SEQUENCE>
																			<STEP>
																				<STMT>
																					<SEND>
																						<NAME VALUE="ACK" />
																					</SEND>
																				</STMT>
																			</STEP>
																		</SEQUENCE>
																	</OPTION>
																	<ELSE>
																		<SEQUENCE>
																			<STEP>
																				<STMT>
																					<EXPR>
																						<CONST VALUE="1,1" />
																					</EXPR>
																				</STMT>
																			</STEP>
																		</SEQUENCE>
																	</ELSE>
																</IF>
															</STMT>
														</STEP>
													</SEQUENCE>
												</OPTION>
												<ELSE>
													<SEQUENCE>
														<STEP>
															<STMT>
																<EXPR>
																	<CONST VALUE="1,1" />
																</EXPR>
															</STMT>
														</STEP>

													</SEQUENCE>
												</ELSE>
											</IF>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>


							<OPTION>
								<SEQUENCE>
									<STEP>
										<STMT>
											<RECEIVE>
												<NAME VALUE="SYNC2" />
											</RECEIVE>
										</STMT>
									</STEP>
									<STEP>
										<STMT>
											<IF>
												<OPTION>
													<EXPR>
														<VARREF>
															<NAME VALUE="RECVD2" />
														</VARREF>
														<OP VALUE="==" />
														<VARREF>
															<NAME VALUE="1,0" />
														</VARREF>
													</EXPR>
													<SEQUENCE>
														<STEP>
															<STMT>
																<ASSIGN>
																	<VARREF>
																		<NAME VALUE="RECVD2" />
																	</VARREF>
																	<EXPR>
																		<VARREF>
																			<NAME VALUE="1,1" />
																		</VARREF>
																	</EXPR>
																</ASSIGN>
															</STMT>
														</STEP>
														<STEP>
															<STMT>
																<IF>
																	<OPTION>
																		<EXPR>
																			<VARREF>
																				<NAME VALUE="RECVD1" />
																			</VARREF>
																			<OP VALUE="==" />
																			<VARREF>
																				<NAME VALUE="1,1" />
																			</VARREF>
																		</EXPR>
																		<SEQUENCE>
																			<STEP>
																				<STMT>
																					<SEND>
																						<NAME VALUE="ACK" />
																					</SEND>
																				</STMT>
																			</STEP>
																		</SEQUENCE>
																	</OPTION>
																	<ELSE>
																		<SEQUENCE>
																			<STEP>
																				<STMT>
																					<EXPR>
																						<CONST VALUE="1,1" />
																					</EXPR>
																				</STMT>
																			</STEP>
																		</SEQUENCE>
																	</ELSE>
																</IF>
															</STMT>
														</STEP>
													</SEQUENCE>
												</OPTION>
												<ELSE>
													<SEQUENCE>
														<STEP>
															<STMT>
																<EXPR>
																	<CONST VALUE="1,1" />
																</EXPR>
															</STMT>
														</STEP>

													</SEQUENCE>
												</ELSE>
											</IF>
										</STMT>
									</STEP>
								</SEQUENCE>
							</OPTION>
						</DO>
					</STMT>
				</STEP>
			</SEQUENCE>
		</PROCTYPE>
	</MODULE>

</SPEC>